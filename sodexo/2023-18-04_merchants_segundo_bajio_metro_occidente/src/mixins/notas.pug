mixin BOX_TEXT(bgColor, textTitle, textContent)
    table(border="0", cellpadding="0", cellspacing="0", width="100%", role="presentation" bgcolor=`${bgColor}` style="max-width: 600px; mso-table-lspace: 0; mso-table-rspace: 0;")
        tr  
            td(width="24")
            td
                table(border="0", cellpadding="0", cellspacing="0", width="100%", role="presentation" style="max-width: 552px; mso-table-lspace: 0; mso-table-rspace: 0;")
                    tr: td(height="32" colspan="3")
                    tr
                        td(width="48")
                        td(align="center" style="font-family: helvetica, arial, sans-serif; line-height: 1; mso-line-height-alt: 100%;")
                            strong(style=`font-size: 24px; color: #FFFFFF;`) !{`${textTitle}`}
                            <br><br>
                            span(style=`font-size: ${textContent}; color: #FFFFFF;`) !{`${textContent}`}
                        td(width="48")
                    tr: td(height="32" colspan="3")
            td(width="24")

