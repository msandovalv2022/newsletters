        tr  
            td(valign="center" align="left", style="font-size: 0.001px; line-height: 1; mso-line-height-alt: 1%;")
                img(src=`${imgThumb}` alt=`${textTitle}` width="100%" style="max-width: 320px;")
            td(valign="center")
                table(border="0" cellpadding="0" cellspacing="0" width="100%" role="presentation" style="max-width: 280px; mso-table-lspace: 0; mso-table-rspace: 0;")
                    tr
                        td(width="24")
                        td(align="left", style="font-family: helvetica, arial, sans-serif; line-height: 1; mso-line-height-alt: 100%;")
                            strong(style=`font-size: 22px; color: ${textTitleColor};`) !{`${textTitle}`}
                            <br><br>
                            span(style=`font-size: ${textBody}; color: #000000;`) !{`${textContent}`}
                            <br><br>
                            a(href=`${imgButtonUrl}` style=`font-size: ${textBody}; color: #000000;`) 
                                img(src=`${imgButton}`, alt=`${imgButtonAlt}` width=`${imgButtonWith}` style=`width: ${imgButtonWidth}px;`)
                        td(width="24")

