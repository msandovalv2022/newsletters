mixin NOTA(arrNotas)
    - let arr = arrNotas
    table(border="0" cellpadding="0" cellspacing="0" width="100%" role="presentation" style="max-width: 600px; mso-table-lspace: 0; mso-table-rspace: 0;")
        each item in arr
            tr  
                td(width="24")
                td
                    table(border="0" cellpadding="0" cellspacing="0" width="100%" bgcolor=`${item.colorBg}` role="presentation" style="max-width: 552px; border-radius: 16px; mso-table-lspace: 0; mso-table-rspace: 0;")
                        tr: td(height="24", colspan="3")
                        tr
                            td(width="24")
                            td
                                table(border="0", cellpadding="0", cellspacing="0", width="auto", role="presentation" style="max-width: 504px; mso-table-lspace: 0; mso-table-rspace: 0;")
                                    tr
                                        td(align="left", style="font-size: 0.001px; line-height: 1; mso-line-height-alt: 1%;")
                                            img(src=`${pathImage}${item.imgLabel}`, alt=`${item.imgLabelAlt}`)
                                    tr
                                        td(style="font-family: helvetica, arial, sans-serif; line-height: 1; mso-line-height-alt: 100%;")
                                            <br><br>
                                            strong(style=`font-size: ${textTitulo}; color: ${item.colorTitle};`) !{`${item.textTitle}`}
                                            <br><br>
                                    tr
                                        td(align="center", style="font-size: 0.001px; line-height: 1; mso-line-height-alt: 1%;")
                                            img(src=`${pathImage}${item.imgThumb}`, style="max-width: 504px; height: auto;" width="100%", height="auto", alt=`${item.imgThumbAlt}`)
                                    tr
                                        td(style="font-family: helvetica, arial, sans-serif; line-height: 1; mso-line-height-alt: 100%;")
                                            <br><br>
                                            span(style=`font-size: ${textBody}; color: #000000;`) !{`${item.textContent}`}
                                            <br><br>
                                    tr
                                        td(align="right", style="font-size: 0.001px; line-height: 1; mso-line-height-alt: 1%;")
                                            a(href=`${item.imgButtonUrl}` rel="noopener" target="_blank") 
                                                img(src=`${pathImage}${item.imgButton}`, alt=`${item.imgButtonAlt}`)
                            td(width="24")
                        tr: td(height="24", colspan="3")
                td(width="24")
            tr: td(height="24", colspan="3")

